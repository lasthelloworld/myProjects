{
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "chrome",
            "request": "launch",
            "name": "使用本机Chrome调试",
            "file": "${workspaceRoot}/index.html",
            //"url": "http://localhost:8080",//使用外部服务器时,请注释掉 file, 改用 url,并将 useBuildInServer 设置为 fals
